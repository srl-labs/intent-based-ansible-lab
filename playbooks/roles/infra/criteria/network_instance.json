{
    "type": "object",
    "patternProperties": {
        "^.*" : {
            "type": "object",
            "properties": {
                "mac_learning": {
                    "type": "object",
                    "properties": {
                        "admin_state": {
                            "type": "string",
                            "enum": ["enable", "disable"]
                        },
                        "age_time": {
                            "type": "number"
                        }
                    }
                },
                "mac_limit": {
                    "type": "number"
                },
                "proxy_arp": {
                    "type": "object",
                    "properties": {
                        "admin_state": {
                            "type": "string",
                            "enum": ["enable", "disable"]
                        },
                        "age_time": {
                            "type": "number"
                        }
                    }
                },
                "ip_duplication": {
                    "type": "object",
                    "properties": {
                        "mon_window": {
                            "type": "number"
                        },
                        "num_moves": {
                            "type": "number"
                        },
                        "hold_down_time": {
                            "type": "number"
                        }
                    }
                },
                "mac_duplication": {
                    "type": "object",
                    "properties": {
                        "mon_window": {
                            "type": "number"
                        },
                        "num_moves": {
                            "type": "number"
                        },
                        "hold_down_time": {
                            "type": "number"
                        },
                        "action": {
                            "type": "string",
                            "enum": ["blackhole", "oper-down", "stop-learning"]
                        }
                    }
                },
                "interfaces": {
                    "type": "array",
                    "items": {
                        "type": "string"
                        }
                    },
                "autonomous-system": {
                    "type": "number"
                    },
                "router-id": {
                    "type": "string",
                    "format": "ipv4"
                    },
                "protocols": {
                    "type": "object",
                    "properties": {
                        "bgp": {
                            "type": "object",
                            "properties": {
                                "dynamic": {
                                    "type": "object",
                                    "properties": {
                                        "interfaces": {
                                            "type": "object",
                                            "patternProperties": {
                                                ".+": {
                                                    "type": "object",
                                                    "required": ["allow-as", "peer-group"],
                                                    "properties": {
                                                        "allow-as": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "number"
                                                            }
                                                        },
                                                        "peer-group": {
                                                            "type": "string"
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                },
                                "ipv4": {
                                    "type": "object",
                                    "properties": {
                                        "admin_state": {
                                            "type": "string",
                                            "enum": ["enable", "disable"]
                                        }
                                    }
                                },
                                "evpn": {
                                    "type": "object",
                                    "properties": {
                                        "rapid-update": {
                                            "type": "boolean"
                                        }
                                    }
                                },
                                "route-advertisement": {
                                    "type": "object",
                                    "properties": {
                                        "rapid-withdrawal": {
                                            "type": "boolean"
                                        }
                                    }
                                },
                                "group": {
                                    "type": "object",
                                    "patternProperties": {
                                        "^.*": {
                                            "type": "object",
                                            "properties": {
                                                "peer_as": {
                                                    "type": "number"
                                                },
                                                "enable_bfd": {
                                                    "type": "boolean"
                                                },
                                                "fast_failover": {
                                                    "type": "boolean"
                                                },
                                                "local_as": {
                                                    "type": "number"
                                                },
                                                "cluster_id": {
                                                    "type": "string"
                                                },
                                                "local_address": {
                                                    "type": "string",
                                                    "anyOf": [
                                                        {"format": "ipv4"},
                                                        {"format": "ipv6"}
                                                    ]
                                                },
                                                "export_policy": {
                                                    "type": "string"
                                                },
                                                "import_policy": {
                                                    "type": "string"
                                                },
                                                "ipv4": {
                                                    "type": "object",
                                                    "properties": {
                                                        "admin_state": {
                                                            "type": "string",
                                                            "enum": ["disable", "enable"]
                                                        }
                                                    }
                                                },
                                                "evpn": {
                                                    "type": "object",
                                                    "properties": {
                                                        "admin_state": {
                                                            "type": "string",
                                                            "enum": ["disable", "enable"]
                                                        }
                                                    }
                                                },
                                                "timers": {
                                                    "type": "object",
                                                    "properties": {
                                                        "connect-retry": {
                                                            "type": "number",
                                                            "minimum": 1,
                                                            "maximum": 65535
                                                        },
                                                        "hold-time": {
                                                            "oneOf": [
                                                                {"const": 0},
                                                                {"minimum": 3, "maximum": 65535}
                                                            ],
                                                            "type": "number"
                                                        },
                                                        "keepalive-interval": {
                                                            "type": "number",
                                                            "minimum": 0,
                                                            "maximum": 21845
                                                        },
                                                        "minimum-advertisement-interval": {
                                                            "type": "number",
                                                            "minimum": 1,
                                                            "maximum": 255
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                },
                                "neighbor": {
                                    "type": "object",
                                    "additionalProperties": false,
                                    "patternProperties": {
                                        "^([0-9]{1,3}\\.){3}[0-9]{1,3}$": {
                                            "type": "object",
                                            "properties": {
                                                "peer_group": {
                                                    "type": "string"
                                                }
                                            },
                                            "required": ["peer_group"]
                                        },
                                        "^(((?:[0-9a-f]{1,4}:){7}[0-9a-f]{1,4})|((?:[0-9a-f]{1,4}:){1,6}:|(?:[0-9a-f]{1,4}:){1,5}(:[0-9a-f]{1,4}){1,2}|(?:[0-9a-f]{1,4}:){1,4}(:[0-9a-f]{1,4}){1,3}|(?:[0-9a-f]{1,4}:){1,3}(:[0-9a-f]{1,4}){1,4}|(?:[0-9a-f]{1,4}:){1,2}(:[0-9a-f]{1,4}){1,5}|[0-9a-f]{1,4}:((:[0-9a-f]{1,4}){1,6})|:((:[0-9a-f]{1,4}){1,7}|:)|fe80:(:[0-9a-f]{0,4}){0,4}%[0-9a-zA-Z]{1,}|::(ffff(:0{1,4}){0,1}:){0,1}((25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9]).){3}(25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])))$": {
                                            "type": "object",
                                            "properties": {
                                                "peer_group": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        },
                        "ospf": {
                            "type": "object",
                            "patternProperties": {
                                "^.*": {
                                    "type": "object",
                                    "required": ["version", "export_policy"],
                                    "properties": {
                                        "version": {
                                            "type": "string",
                                            "enum": ["ospf-v2"]
                                        },
                                        "export_policy": {
                                            "type": "string"
                                        },
                                        "area": {
                                            "type": "object",
                                            "patternProperties": {
                                                "^([0-9]{1,3}\\.){3}[0-9]{1,3}$": {
                                                    "type": "object",
                                                    "properties": {
                                                        "interfaces": {
                                                            "type": "object",
                                                            "patternProperties": {
                                                                "^.*": {
                                                                    "type": "object",
                                                                    "properties": {
                                                                        "passive": {
                                                                            "type": "boolean"
                                                                        },
                                                                        "interface-type": {
                                                                            "type": "string",
                                                                            "enum": ["point-to-point", "broadcast"]
                                                                        }
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
